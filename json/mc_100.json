        "scopecontent": {
            "head": "Scope and Contents",
            "p": {
                "value": "This collection is comprised of correspondence and questionnaires collected by Oscar Rose\n        between 1935 and 1938 for a proposed biography of John \"\"the Cop\" Quigley. Quigley was a\n        senior patrolman and guardian of the University Heights campus. The correspondence and\n        questionnaires pertain to both John Quigley and life at the University Heights campus of New\n        York University."
            }
        },
        "bioghist": {
            "head": "Historical note",
            "p": {
                "value": "The University Heights campus, located in the Bronx, was a male-only undergraduate campus.\n        It was opened in the late nineteenth-century and sold in 1973."
            }
        },
        "accessrestrict": {
            "head": "Conditions Governing Access",
            "p": {
                "value": "Materials are open without restrictions."
            }
        },
        "userestrict": {
            "head": "Conditions Governing Use",
            "p": {
                "value": "Any rights (including copyright and related rights to publicity and privacy) held by Oscar\n        Rose are maintained by New York University. Permission to publish or reproduce materials in\n        this collection must be secured from repository. Please contact the New York University\n        Archives at university-archives@nyu.edu."
            }
        },
